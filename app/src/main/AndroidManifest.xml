<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.spotify_clone_main">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".UI.LoginAct"

            />
        <activity android:name=".UI.SignUpAct"

            />
        <activity
            android:name=".UI.spotify"
            android:label="@string/title_activity_spotify">
        </activity>
        <activity
            android:name=".UI.youtube"
            android:label="@string/title_activity_youtube">

        </activity>
        <activity android:name=".UI.Platform_choose" />
        <activity android:name=".UI.MainActivity"
          >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>